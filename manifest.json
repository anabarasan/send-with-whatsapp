{
  "manifest_version": 3,
  "name": "Send via Whatsapp",
  "description": "Send WhatsApp message without saving contact",
  "version": "1.1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}

